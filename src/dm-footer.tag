    <div class="area-copyright">
        <a href="https://danmaid.com/">
            <i class="far fa-copyright"></i> 2018 男メイド</a>
    </div>

    <div class="modal" id="modal" onclick="{ modalClose }">
        <div class="modal-window" id="modal-window" onclick="{ noneAction }">
        </div>
    </div>

    <style>
        :scope {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
            background-color: rgb(38, 38, 42);
            color: rgb(209, 209, 231);
        }

        .area-copyright {
            display: flex;
            justify-content: flex-end;
            align-items: flex-end;
            text-align: end;
        }

        .modal {
            display: none;
            overflow: hidden;
            position: fixed;
            left: 0;
            top: 0;
            z-index: 10000;
        }

        .modal.enabled {
            width: 100vw;
            height: 100vh;
            background-color: rgba(0, 0, 0, 0.7);
            display: flex;
            align-items: center;
            justify-content: center;
        }

        .modal .modal-window {
            border-radius: 16px;
            padding: 16px;
            background-color: rgb(220, 220, 240);
            color: rgb(19, 19, 21);
            position: relative;
            overflow: hidden;
        }

        .modal .modal-title {
            box-sizing: border-box;
            width: 100%;
            padding: 16px;
            background-color: rgb(19, 19, 21);
            color: rgb(220, 220, 240);
        }

        .modal .modal-content {
            padding: 16px;
        }

        .qrcode {
            height: 200px;
            margin: 4px;
        }

        .contact-list,
        .donation-list {
            display: flex;
            bottom: 0;
            left: 0;
            list-style: none;
            padding: 0;
            margin: 8px;
            width: calc(100% - 8px - 8px);
            line-height: 32px;
        }

        .contact-list .contact,
        .donation-list .contact {
            background-color: rgba(70, 70, 70, 1);
            width: 32px;
            height: 32px;
            font-size: 16px;
            border-radius: 50%;
            margin-left: 8px;
        }

        .donation-list .contact {
            border-radius: 20%;
            font-size: 20px;
        }

        .contact-list a,
        .donation-list a {
            width: 100%;
            height: 100%;
            display: flex;
            justify-content: center;
            align-items: center;
            color: inherit;
        }

        .contact-list .contact:hover,
        .donation-list .contact:hover {
            background-color: rgba(153, 153, 153, 1);
            color: rgba(34, 34, 34, 1);
        }
    </style>

    <script>
        this.noneAction = (e) => {
            e.stopPropagation()
        }

        this.modalOpen = (target) => {
            console.log('open:', target)
            let root = document.getElementById('modal')
            let content = document.getElementById(target).content.cloneNode(true)
            let window = root.querySelector('#modal-window')
            window.appendChild(content)
            root.classList.add('enabled')
        }

        this.modalClose = () => {
            console.log('close')
            let root = document.getElementById('modal')
            let window = root.querySelector('#modal-window')
            window.textContent = null
            root.classList.remove('enabled')
        }
    </script>
</dm-footer>